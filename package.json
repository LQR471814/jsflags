{
  "name": "jsflags",
  "version": "1.1.1",
  "description": "An extremely simple yet flexible JavaScript library for parsing command-line flags, inspired by the golang \"flags\" library.",
  "files": [
    "./dist/**"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs",
      "default": "./dist/*.mjs"
    }
  },
  "scripts": {
    "lint": "biome check . --apply-unsafe",
    "prepare": "biome check . --apply-unsafe",
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "flags",
    "cli"
  ],
  "author": "LQR471814",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@swc/core": "1.3.99",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.10",
    "jest": "29.7.0",
    "tsup": "^8.0.1",
    "typescript": "5.3.2"
  }
}
